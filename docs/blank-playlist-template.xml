<!-- 
  BLANK PLAYLIST TEMPLATE FOR AUTO-MUSICL-MAKER
  Repository: https://github.com/ChadFarrow/Auto-musicL-Maker
  
  Instructions:
  1. Replace [AUTHOR_NAME] with your name or organization
  2. Replace [PLAYLIST_NAME] with your playlist title
  3. Replace [DESCRIPTION] with your playlist description  
  4. Replace [WEBSITE_URL] with the source website URL
  5. Replace [SOURCE_FEED_URL] with the RSS feed URL
  6. Replace [CURRENT_UTC_DATE] with current date in UTC format
  7. Replace [GENERATED_UUID] with a unique UUID (use online UUID generator)
  8. Replace [FEED_GUID] and [ITEM_GUID] with actual GUIDs from your source feed
  9. Add more <podcast:remoteItem> entries as needed
  10. Optionally add an image URL in the <image><url></url> section
-->
<rss version="2.0" xmlns:podcast="https://podcastindex.org/namespace/1.0">
  <channel>
    <author>[AUTHOR_NAME]</author>
    <title>[PLAYLIST_NAME] Music Playlist</title>
    <description>[DESCRIPTION]</description>
    <link>[WEBSITE_URL]</link>
    <podcast:txt purpose="source-feed">[SOURCE_FEED_URL]</podcast:txt>
    <language>en</language>
    <pubDate>[CURRENT_UTC_DATE]</pubDate>
    <lastBuildDate>[CURRENT_UTC_DATE]</lastBuildDate>
    <image>
      <url></url>
    </image>
    <podcast:medium>musicL</podcast:medium>
    <podcast:guid>[GENERATED_UUID]</podcast:guid>
    <podcast:remoteItem feedGuid="[FEED_GUID]" itemGuid="[ITEM_GUID]" />
    <!-- Add more remote items as needed -->
  </channel>
</rss>