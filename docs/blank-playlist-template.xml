<!-- 
  BLANK PLAYLIST TEMPLATE FOR AUTO-MUSICL-MAKER
  Repository: https://github.com/ChadFarrow/Auto-musicL-Maker
  
  See example playlist: https://github.com/ChadFarrow/Auto-musicL-Maker/blob/main/docs/doerfelverse-playlist-example.xml
  
  Instructions:
  1. Replace [AUTHOR_NAME] with your name or organization
  2. Replace [PLAYLIST_NAME] with your playlist title
  3. Replace [DESCRIPTION] with your playlist description  
  4. Replace [WEBSITE_URL] with the source website URL
  5. Replace [SOURCE_FEED_URL] with the RSS feed URL
  6. Replace [LANGUAGE_CODE] with your language code (e.g., 'en', 'es', 'fr', 'de')
  7. Replace [CURRENT_UTC_DATE] with current date in UTC format (when the feed was created)
  8. Replace [IMAGE_URL] with your playlist image URL (optional - leave empty if no image)
  9. Replace [GENERATED_UUID] with a unique UUID (use online UUID generator)
  10. Replace [FEED_GUID] and [ITEM_GUID] with actual GUIDs from your source feed
  11. Add more <podcast:remoteItem> entries as needed
-->
<rss version="2.0" xmlns:podcast="https://podcastindex.org/namespace/1.0">
  <channel>
    <author>[1-AUTHOR_NAME]</author>
    <title>[2-PLAYLIST_NAME]</title>
    <description>[3-DESCRIPTION]</description>
    <link>[4-WEBSITE_URL]</link>
    <podcast:txt purpose="source-feed">[5-SOURCE_FEED_URL]</podcast:txt>
    <language>[6-LANGUAGE_CODE]</language>
    <pubDate>[7-CURRENT_UTC_DATE]</pubDate>
    <lastBuildDate>[7-CURRENT_UTC_DATE]</lastBuildDate>
    <image>
      <url>[8-IMAGE_URL]</url>
    </image>
    <podcast:medium>musicL</podcast:medium>
    <podcast:guid>[9-GENERATED_UUID]</podcast:guid>
    <podcast:remoteItem feedGuid="[10-FEED_GUID]" itemGuid="[10-ITEM_GUID]" />
    <!-- Add more remote items as needed -->
  </channel>
</rss>